    wall-2d_v0_boss-extrude1
    {
        type            empty;
    }

    inlet
    {
        type            fixedValue;
        value           uniform 3.35410197e-03;  // nu_t â‰ˆ C_mu * k^2 / epsilon (example)
    }

    outlet
    {
        type            zeroGradient;
    }

    outerwall
    {
        type            nutkWallFunction;
