FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (1 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (1 0 0);
    }

    wall
    {
        type            noSlip;
    }

    outlet
    {
        type            zeroGradient; 
    }
	
    wall-2d_v0_boss-extrude1  //"(front|back)"
    {
        type            empty; 
    }
}


